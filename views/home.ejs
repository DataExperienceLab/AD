          })                   
      
          socket.on('E6', function(message) {
      console.log(message)
            $("#e6").text(JSON.stringify(message, undefined, 4));
          })      
      launch=function(){
      
          var ee = $("#te").val()
          console.log(ee)
           socket.emit('MOTOR_ID', ee);
       }
      launch2=function(){
      
      
           socket.emit('SOS',"OK");
       }
      
      });
       
